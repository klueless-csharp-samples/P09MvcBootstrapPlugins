@{
    ViewData["Title"] = "JQuery Table";
}

<div class="text-center">
    <h1 class="display-4">JQuery Advanced Table</h1>
</div>


<table
  id="table"
  data-toggle="table"
  data-flat="true"
  data-search="true"
  data-url="../db.json">
  <thead>
    <tr>
      <th data-field="name" data-sortable="true">Name</th>
      <th data-field="stargazers_count" data-sortable="true">Stargazers</th>
      <th data-field="forks_count" data-sortable="true">Forks</th>
      <th data-field="description" data-sortable="true">Description</th>
    </tr>
  </thead>
</table>